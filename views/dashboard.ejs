<%- include('partials/header')%>
<body>
  <header>
    <h1>Hello <%=UK.username%></h1>
  </header>
  <div id="form">
  
    <div class="fish" id="fish"></div>
    <div class="fish" id="fish2"></div>
    
    <form id="waterform" action="/logout"method="post">
    <div class="formgroup" id="name-form">
        <label for="username">Username*</label>
        <input type="text"id="username"style="
        font-size: 1.5em;color:white;background-color:transparent"value='<%=UK.username%>'readonly/>
    </div>
    
    <div class="formgroup" id="email-form">
        <label for="email">Email*</label>
        <input type="email" id="email"style="
        font-size: 1.5em;color:white;background-color:transparent"value='<%=UK.email%>'readonly/>
    </div>
    <button type="submit" class="w-100 mt-5 btn-primary btn btn-lg btn-block">
        Logout Now</button>    
    </div>
  <%- include('partials/footer')%>